set completion-query-items 250
set colored-stats
set colored-completion-prefix
set show-mode-in-prompt
set skip-completed-text
set editing-mode vi
set vi-cmd-mode-string \1\e[1;38;5;226m\2:\1\e[0m\2
set vi-ins-mode-string \1\e[1;38;5;214m\2+\1\e[0m\2
Control-f: menu-complete
Control-g: menu-complete-backward
Control-a: complete-filename
Control-l: insert-completions
Control-e: forward-search-history
