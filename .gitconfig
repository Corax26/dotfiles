	default = upstream
[merge]
	defaultToUpstream = true
	tool = gvimdiff
[core]
	excludesfile = ~/.gitignore_global
[i18n]
	logOutputEncoding = UTF-8
[color]
	ui = auto
[pager]
	grep = false
